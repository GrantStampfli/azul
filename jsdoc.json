{
  "source": {
    "include": ["lib"]
  },
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "recurse": true
  },
  "plugins": [ "plugins/markdown" ],
  "tags"      : {
    "allowUnknownTags" : true
  },
  "templates" : {
    "cleverLinks"           : false,
    "monospaceLinks"        : false,
    "dateFormat"            : "ddd MMM Do YYYY",
    "outputSourceFiles"     : true,
    "outputSourcePath"      : true,
    "systemName"            : "Azul",
    "footer"                : "",
    "copyright"             : "Azul Copyright Â© 2014 Whitney Young.",
    "navType"               : "vertical",
    "theme"                 : "simplex",
    "linenums"              : false,
    "collapseSymbols"       : false,
    "inverseNav"            : true,
    "customCSS": "hello",
    "highlightTutorialCode" : true,
    "protocol": "fred://"
  },
  "markdown"  : {
    "parser"   : "gfm",
    "hardwrap" : true
  }
}
